<h1>Listing events</h1>

<table>
  <tr>
    <th>Web page url</th>
    <th>Image url</th>
    <th>Video url</th>
    <th>Audio url</th>
    <th>Loc latitude</th>
    <th>Loc longitude</th>
    <th>Date</th>
    <th>Contact</th>
    <th>Price</th>
    <th>Event url</th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%=h event.web_page_url %></td>
    <td><%=h event.image_url %></td>
    <td><%=h event.video_url %></td>
    <td><%=h event.audio_url %></td>
    <td><%=h event.loc_latitude %></td>
    <td><%=h event.loc_longitude %></td>
    <td><%=h event.date %></td>
    <td><%=h event.contact %></td>
    <td><%=h event.price %></td>
    <td><%=h event.event_url %></td>
    <td><%= link_to 'Show', customer_event_path(params[:customer_id],event) %></td>
    <td><%= link_to 'Edit', edit_customer_event_path(params[:customer_id],event) %></td>
    <td><%= link_to 'Destroy', event, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New event', new_customer_event_path(params[:id]) %>